<?xml version="1.0" encoding="ISO-8859-1" ?>
<install>

        <!-- Define the main module properties -->
        <module_properties>
            <name>faq_category</name>
            <parent>faq</parent>
            <notes>This module controls the FAQs categories</notes>
            <menu_display>1</menu_display>
            <dependancy></dependancy>
            <sub_modules></sub_modules>
        </module_properties>


        <!-- Define any methods for this module -->
        <sql_inserts>
            <module_method>
                <search>
                    <name>search</name>
                </search>
                <view>
                    <name>view</name> 
                    <page><![CDATA[core:search&module=%%&_escape=1&_next_page_one=view]]></page>
                    <menu_display>1</menu_display>
                </view>
                <add>
                    <name>add</name> 
                    <menu_display>1</menu_display>
                </add>
                <delete>
                    <name>delete</name>
                </delete>
                <update>
                    <name>update</name>
                </update>
                <search_show>
                    <name>search_show</name> 
                </search_show>
            </module_method>
        </sql_inserts>
</install>
